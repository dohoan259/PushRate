<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="RateDialogStyle" parent="Theme.MaterialComponents.DayNight.Dialog.Alert">
        <item name="android:fontFamily">@font/quicksand_bold</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="SettingTitle">
        <item name="android:textSize">14dp</item>
        <item name="android:fontFamily">@font/quicksand_bold</item>0
        <item name="android:textColor">@color/blueDark1</item>
    </style>

    <style name="SettingValue">
        <item name="android:textSize">12dp</item>
        <item name="android:fontFamily">@font/quicksand_regular</item>
        <item name="android:textColor">@color/blueDark1</item>
        <item name="android:textStyle">bold</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
        <item name="singleLine">true</item>
    </style>
</resources>